<?xml version="1.0" encoding="UTF-8"?>
<web-app
  version="4.0"
  xmlns="http://xmlns.jcp.org/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee  http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">

  <display-name>JsonOracleStorage</display-name>
  <description>A simple Servlet</description>
  
  <resource-ref>
    <res-ref-name>jndi/example-properties</res-ref-name>
    <res-type>java.util.Properties</res-type>
    <lookup-name>java:app/jndi/example-properties</lookup-name>
  </resource-ref>
  
  <resource-ref>
    <res-ref-name>jdbc/myOracleDataSource</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
    <lookup-name>jdbc/myOracleDataSource</lookup-name>
  </resource-ref>
  
  <welcome-file-list>
	<welcome-file>index.html</welcome-file>
  </welcome-file-list>
    
</web-app>
